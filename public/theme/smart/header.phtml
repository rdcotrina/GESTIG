                setTimeout(function () {
                    $("#search-fld").autocomplete({
                        source: SYS_MENU,
                        select: function (event, ui) {
                            this.alias = ui.item.alias;
                            this.root = ui.item.root;

                            var t = '&nbsp;&nbsp;&nbsp;&nbsp;' + ui.item.title;
                            var e = ui.item.evento;
                            if ($('#li-' + ui.item.alias).length) {
                                $('#li-' + ui.item.alias).remove();
                            }
                            eval(e);
                            /*una vez insertado el titulo se detendra el interval*/
                            var interval = setInterval(function () {
                                if ($('#li-' + ui.item.alias).length) {
                                    $('#li-' + ui.item.alias).find('a').html(t);
                                    clearInterval(interval);
                                }
                            }, 10);
                        }
                    });
                }, 1000);
